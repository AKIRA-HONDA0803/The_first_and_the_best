<div class='row'>
<%  @user.each do |user| %>
<div class="card w-50 mx-auto shadow-lg col-3">
   <div class="card-body">

   <%= link_to user_path(user.id) do%>
   <%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %>
  <p><%= user.name %></p>
  <% end %>
</div>
</div>
<% end %>
</div>